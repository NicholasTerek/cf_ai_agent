{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "strict": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "lib": ["ES2022", "WebWorker"],
    "types": ["@cloudflare/workers-types/2023-07-01"]
  },
  "include": ["src/**/*", "wrangler.toml"]
}
